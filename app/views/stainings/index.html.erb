<table class="table table_striped table-bordered">
  <thead>
    <tr>
      <th>Zarodek</th>
      <th>Data barwienia</th>
      <th>Przeciwciała</th>
      <th>Protokół barwienia</th>
      <th>Wyniki</th>
      <th>Plik z wynikami</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stainings.each do |staining| %>
      <tr>
        <td><%= staining.foetus %></td>
        <td><%= staining.staining_date %></td>
        <td><%= staining.antibodies %></td>
        <td><%= staining.staining_protocol %></td>
        <td><%= staining.results %></td>
        <td><%= staining.results_file %></td>
        <td><%= link_to 'Szczegóły', staining %></td>
        <td><%= link_to 'Edytuj', edit_staining_path(staining) %></td>
        <td><%= link_to 'Usuń', staining, method: :delete, data: { confirm: 'Na pewno usunąć?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nowe barwienie', new_staining_path, class: "btn btn-info btn-sm" %>
