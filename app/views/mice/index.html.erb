<table class="table table_striped table-bordered">
  <thead>
    <tr>
      <th>Mysz nr F1#</th>
      <th>Data krycia</th>
      <th>DPC</th>
      <th>Somity</th>
      <th>Doświadczenia</th>
      <th>Uwagi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mice.each do |mouse| %>
      <tr>
        <td><%= mouse.number %></td>
        <td><%= mouse.mating_date %></td>
        <td><%= mouse.dpc %></td>
        <td><%= mouse.somites %></td>
        <td><%= mouse.experiments %></td>
        <td><%= mouse.notes %></td>
        <td><%= link_to 'Szczegóły', mouse %></td>
        <td><%= link_to 'Edytuj', edit_mouse_path(mouse) %></td>
        <td><%= link_to 'Usuń', mouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br/>

<%= link_to 'Dodaj mysz', new_mouse_path, class: "btn btn-info btn-sm" %>
