<table class="table table_striped table-bordered">
  <thead>
    <tr>
      <th>Tytuł</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protocols.each do |protocol| %>
      <tr>
        <td><%= protocol.title %></td>
        <td><%= link_to 'Pokaż', protocol %></td>
        <td><%= link_to 'Edytuj', edit_protocol_path(protocol) %></td>
        <td><%= link_to 'Usuń', protocol, method: :delete, data: { confirm: 'Na pewno usunąć?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Dodaj protokół', new_protocol_path, class: "btn btn-info btn-sm"  %>
