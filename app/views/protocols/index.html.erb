<div class="container">
  <div class="row">
    <div class="col-md-8">
      <table class="table table-striped table-bordered table-condensed">
        <thead>
          <tr>
            <th>Tytuł</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @protocols.each do |protocol| %>
            <tr>
              <td><%= protocol.title %></td>
              <td><%= link_to 'Szczegóły', protocol, class: "btn btn-info btn-xs" %>
                  <%= link_to 'Edytuj', edit_protocol_path(protocol), class: "btn btn-warning btn-xs" %>
                  <%= link_to 'Usuń', protocol, method: :delete, data: { confirm: 'Na pewno usunąć?' }, class: "btn btn-danger btn-xs" %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<br>

<%= link_to 'Dodaj protokół', new_protocol_path, class: "btn btn-primary btn-sm"  %>
